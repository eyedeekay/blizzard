clientApp.1.main=net.i2p.util.ShellCommand
clientApp.1.name=snowflake.exe
clientApp.1.args=$PLUGIN/lib/karen.exe -exeperm "0755" -exe $PLUGIN/lib/snowflake.exe -args "-log $PLUGIN/lib/snowflake.log" -exedir $PLUGIN/lib/ -instruct start
clientApp.1.stopargs=$PLUGIN/lib/karen.exe -exe snowflake.exe -exedir $PLUGIN/lib/ -instruct stop
clientApp.1.delay=1
clientApp.1.startOnLoad=true
